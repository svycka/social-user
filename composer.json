{
    "name": "svycka/social-user",
    "description": "This module adds custom grant types for oauth2-server-php to allow login with social services like google or facebook.",
    "keywords": [
        "social login",
        "google",
        "facebook",
        "oauth2",
        "oauth2-server-php",
        "social-user",
        "zendframework",
        "doctrine",
        "zf2",
        "module",
        "zend-expressive"
    ],
    "homepage": "https://github.com/svycka/social-user",
    "license": "MIT",
    "authors": [
        {
            "name": "Vytautas Stankus",
            "email": "svycka@gmail.com",
            "homepage": "https://github.com/svycka"
        }
    ],
    "require": {
        "php": "^5.6 || ^7.0",
        "zendframework/zend-servicemanager": "^2.7.5 || ^3.0.3",
        "bshaffer/oauth2-server-php": "^1.8"
    },
    "require-dev": {
        "doctrine/orm": "^2.5.6",
        "facebook/graph-sdk": "^5.3.1",
        "guzzlehttp/guzzle": "^6.1",
        "phpunit/phpunit": "^4.8",
        "scrutinizer/ocular": "~1.1",
        "squizlabs/php_codesniffer": "^2.5",
        "zendframework/zend-modulemanager": "^2.7"
    },
    "suggest": {
        "facebook/graph-sdk": "Required, if you will use Facebook grant type",
        "guzzlehttp/guzzle": "Required, if you will use Google grant type",
        "doctrine/orm": "Required, if you will use Doctrine for storage",
        "zendframework/zend-modulemanager": "Required, if you will use zend-mvc"
    },
    "autoload": {
        "psr-4": {
            "Svycka\\SocialUser\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Svycka\\SocialUserTest\\": "tests/SocialUserTest"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "zf": {
            "component": "Svycka\\SocialUser",
            "config-provider": "Svycka\\SocialUser\\ConfigProvider"
        }
    }
}
